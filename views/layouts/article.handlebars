<div class="card" href={{articleData.url}}>
  <div class="card-image">
    <figure class="image is-3by2">
      <img src={{articleData.image}} alt="Article Image">
    </figure>
  </div>
  <div class="card-content">
    <div class="media">
      <div class="media-content">
        <p class="title is-4">{{articleData.title}}</p>
      </div>
    </div>
    <div class="content">
      {{articleData.abstract}}
    </div>
  </div>
</div>
<script src="/public/index.js"